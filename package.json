    "quiniela-results": "2.1.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.3.2",
    "standard": "7.1.2",
    "semantic-release": "4.3.5"
  },
  "config": {
